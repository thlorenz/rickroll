{
  "name": "s3-post-metadata",
  "version": "0.1.0",
  "bin": {
    "s3-post-metadata": "bin/cli"
  },
  "scripts": {
    "aws": "aws --endpoint-url=http://localhost:4566",
    "build": "tsc -p tsconfig.json && (cd lambda/upload && yarn build)",
    "cdkr": "cdk",
    "cdk": "cdklocal",
    "up": "docker compose up",
    "build:lambda": "yarn build:upload",
    "build:upload": "(cd lambda/upload && yarn esbuild)",
    "boot": "yarn cdk bootstrap && yarn deploy",
    "deploy": "yarn cdk deploy --require-approval=never"
  },
  "devDependencies": {
    "@types/ansi-colors": "^3.2.2",
    "@types/debug": "^4.1.7",
    "@types/node": "10.17.27",
    "ansi-colors": "^4.1.3",
    "aws-cdk": "2.33.0",
    "aws-cdk-local": "^2.15.0",
    "constructs": "^10.1.51",
    "debug": "^4.3.4",
    "esbuild": "^0.14.49",
    "esbuild-runner": "^2.2.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.33.0"
  }
}
